{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "bookmarks.saveBookmarksInProject": true,
  "breadcrumbs.symbolPath": "off",
  "cSpell.enabled": true,
  "css.validate": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.fontFamily": "Iosevka",
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.quickSuggestions": {
    "comments": "on",
    "strings": "on",
    "other": "on"
  },
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "errorLens.enabledDiagnosticLevels": ["warning", "info", "error"],
  "eslint.packageManager": "yarn",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "vue",
    "typescript",
    "typescriptreact"
  ],
  "explorer.confirmDragAndDrop": false,
  "explorer.incrementalNaming": "smart",
  "extensions.closeExtensionDetailsOnViewChange": true,
  "files.associations": {
    ".huskyrc": "json",
    ".lintstagedrc": "json",
    "*.bash*": "shellscript",
    "*.conf": "shellscript",
    "*.fehbg": "shellscript",
    "*.gql": "graphql",
    "*.smil": "xml",
    "*.vuerc": "json",
    "config": "shellscript"
  },
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.hg": true,
    "**/.next": true,
    "**/.svn": true,
    "**/.vercel": true,
    "**/CVS": true
  },
  "files.simpleDialog.enable": true,
  "git.autofetch": true,
  "git.defaultCloneDirectory": "~/experiment",
  "gitlens.codeLens.enabled": false,
  "gitlens.mode.active": "zen",
  "gitlens.statusBar.enabled": false,
  "javascript.suggest.completeFunctionCalls": true,
  "keyboard.dispatch": "keyCode",
  "liveshare.presence": true,
  "material-icon-theme.activeIconPack": "react",
  "material-icon-theme.files.associations": {
    "_app.page.tsx": "vercel",
    "_document.page.tsx": "html",
    ".lintstagedrc": "eslint",
    ".vuerc": "vue-config",
    "codegen.yml": "graphql",
    "database.rules.json": "database",
    "graphql.config.js": "graphql",
    "storage.rules": "firebase"
  },
  "material-icon-theme.folders.associations": {
    "_site": "dist",
    ".config": "config",
    ".hygen": "layout",
    ".next": "temp",
    ".patches": "git",
    ".vimpress": "vue-directives",
    ".vscode-oss": "vscode",
    ".vuepress": "vue-directives",
    "actions": "api",
    "application": "app",
    "blog": "content",
    "code": "src",
    "contexts": "pipe",
    "cypress": "test",
    "diffs": "batch",
    "enums": "typescript",
    "fixtures": "json",
    "fragments": "graphql",
    "inbox": "mail",
    "integration": "test",
    "media": "video",
    "mutations": "graphql",
    "profile": "admin",
    "queries": "graphql",
    "session": "secure",
    "storybook": "storybook",
    "support": "tools",
    "types": "typescript",
    "verify": "guard"
  },
  "material-icon-theme.folders.color": "#464646",
  "material-icon-theme.hidesExplorerArrows": true,
  "redhat.telemetry.enabled": false,
  "search.exclude": {
    "**/*.code-search": true,
    "**/bower_components": true,
    "**/node_modules": true,
    "package-lock.json": true,
    "yarn.lock": true
  },
  "search.useGlobalIgnoreFiles": true,
  "stylelint.enable": true,
  "tabnine.experimentalAutoImports": true,
  "tailwindCSS.emmetCompletions": true,
  "terminal.integrated.fontFamily": "Iosevka",
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.rendererType": "auto",
  "vetur.completion.scaffoldSnippetSources": {
    "vetur": ""
  },
  "vetur.grammar.customBlocks": {
    "docs": "md",
    "i18n": "json",
    "page-query": "graphql",
    "static-query": "graphql"
  },
  "vetur.validation.templateProps": true,
  "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.customMenuBarAltFocus": false,
  "window.enableMenuBarMnemonics": false,
  "window.menuBarVisibility": "toggle",
  "window.zoomLevel": 2,
  "workbench.editor.untitled.hint": "hidden",
  "workbench.experimental.sidePanel.enabled": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.settings.editor": "json",
  "workbench.startupEditor": "none",
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": false,
  "editor.experimental.stickyScroll.enabled": true,
  "terminal.integrated.profiles.linux": {
    "zsh": {
      "path": "/bin/zsh",
      "args": ["-l", "-i"]
    }
  }
}
