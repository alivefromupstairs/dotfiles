super + Escape
  xkill

super + shift + Escape
  sudo shutdown +0

super + Return
  st

super + grave
  st

super + e
  st -e lf

super + shift + e
  st -e lf /tmp/downloads/

super + l
  slock

XF86Display
  maim --hidecursor --shader=crosshair -s | xclip -selection clipboard -t image/png

shift + XF86Display
  maim -s --hidecursor --shader=crosshair /home/me/media/pix/screenshots/$(date +%s).png

XF86HomePage
  $BROWSER --force-dark-mode

shift + XF86HomePage
  $BROWSER --incognito --force-dark-mode

alt + XF86HomePage
  $BROWSER --guest --force-dark-mode

XF86Calculator
  code

shift + XF86Calculator
  mpv --player-operation-mode=pseudo-gui

super + XF86Calculator
  st -e eva

super + XF86HomePage
  xdg-open $(xclip -o)

super + space
  playerctl play-pause

super + {Left, Right, Down, Up}
  playerctl position {5-, 5+, 60-, 60+}

super + {minus, plus}
  playerctl volume {0.1-, 0.1+}

XF86AudioLowerVolume
  amixer set Master 2-

XF86AudioRaiseVolume
  amixer set Master 2+

XF86MonBrightnessDown
  xbacklight -dec 9

XF86MonBrightnessUp
  xbacklight -inc 10

XF86Explorer
  < ~/.config/sxhkd/regular-text | dmenu -p "type:" -g 2 -l 3 | xargs xdotool type --clearmodifiers

XF86LaunchA
  copyq toggle

XF86WLAN
  sudo systemctl restart NetworkManager.service

super + c
  menurc

#XF86AudioMicMute
  #  notify-send "Volume is $()%"

XF86Tools
  notify-send "Battery capacity is $(< /sys/class/power_supply/BAT0/capacity || echo 'âˆž')%"

XF86Search
  /home/me/.local/bin/clipmenu
