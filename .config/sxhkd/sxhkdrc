super + Escape
  xkill

super + shift + Escape
  sudo shutdown +0

super + Return
  st

super + grave
  st

super + d
  dmenu_run -m "0" -l 5

super + e
  st -e lf

super + shift + e
  st -e lf /tmp/downloads/

super + l
  slock

XF86Display
  maim --hidecursor --shader=crosshair -s | xclip -selection clipboard -t image/png

shift + XF86Display
  maim -s --hidecursor --shader=crosshair /home/me/media/pix/screenshots/$(date +%s).png

XF86HomePage
  $BROWSER

shift + XF86HomePage
  $BROWSER --incognito

alt + XF86HomePage
  $BROWSER --guest

XF86Calculator
  code

shift + XF86Calculator
  st -e musikcube

super + XF86Calculator
  st -e eva

super + XF86HomePage
  xdg-open $(xclip -o)

super + space
  playerctl play-pause

super + {Left, Right, Down, Up}
  playerctl position {5-, 5+, 60-, 60+}

super + {minus, plus}
  playerctl volume {0.1-, 0.1+}

XF86AudioLowerVolume
  pactl set-sink-volume 0 -5% && dwmbar

XF86AudioRaiseVolume
  pactl set-sink-volume 0 +5% && dwmbar

XF86MonBrightnessDown
  xbacklight -dec 9

XF86MonBrightnessUp
  xbacklight -inc 10

XF86Explorer
  < ~/.config/sxhkd/regular-text | dmenu -p "type:" -l 4 | xargs xdotool type --clearmodifiers

XF86LaunchA
  clipmenu

XF86WLAN
  sudo systemctl restart NetworkManager.service

super + c
  menurc
